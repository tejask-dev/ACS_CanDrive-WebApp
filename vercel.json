{
  "version": 2,
  "builds": [
    {
      "src": "acs-can-drive-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "acs-can-drive-frontend/$1"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://your-backend-url.vercel.app/api"
  }
}
